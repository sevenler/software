<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <gradient android:startColor="#00ffffff" android:endColor="#00ffffff" android:angle="270.0" android:centerX="0.55" android:centerY="0.5" android:centerColor="#00ffffff" />
        </shape>
    </item>
    <item android:state_pressed="true" android:drawable="@drawable/one_key_widget_light" />
    <item android:state_focused="true" android:drawable="@drawable/one_key_widget_light" />
</selector>