<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <clip>
            <shape>
                <corners android:radius="15.0dip" />
                <gradient android:startColor="#ffdbdbdb" android:endColor="#ffdbdbdb" android:angle="270.0" android:centerY="0.75" />
                <stroke android:width="1.0dip" android:color="#ffaeaeae" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="15.0dip" />
                <gradient android:startColor="#ffbaff00" android:endColor="#ff005f09" android:angle="270.0" android:centerY="0.75" />
                <stroke android:width="1.0dip" android:color="#ffaeaeae" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#fffedc1d" android:endColor="#fffedc1d" android:angle="270.0" android:centerY="0.25" />
                <corners android:radius="15.0dip" />
                <stroke android:width="1.0dip" android:color="#fffedc1d" />
            </shape>
        </clip>
    </item>
</layer-list>